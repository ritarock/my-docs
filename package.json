{
  "name": "my-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint pages/** components/** lib/**",
    "lint-fix": "eslint --fix pages/ components/ lib/",
    "lint-md": "textlint docs/**/*.md",
    "build-md": "rm articles/* && processmd 'docs/*.md' --outputDir ./articles --summaryOutput ./articles/summary.json",
    "format": "prettier --write components/*.tsx lib/*.ts pages/**/*.tsx"
  },
  "dependencies": {
    "next": "10.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2",
    "react-syntax-highlighter": "^15.4.3"
  },
  "devDependencies": {
    "@types/react": "17.0.8",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "postcss": "^8.3.2",
    "prettier": "2.3.1",
    "processmd": "^4.6.1",
    "remark-gfm": "^1.0.0",
    "tailwindcss": "^2.1.4",
    "textlint": "^12.0.0",
    "textlint-rule-ja-no-mixed-period": "^2.1.1",
    "textlint-rule-no-doubled-joshi": "^4.0.0",
    "textlint-rule-no-dropping-the-ra": "^3.0.0",
    "textlint-rule-no-mix-dearu-desumasu": "^5.0.0",
    "typescript": "4.3.2"
  }
}
